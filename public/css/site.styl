.m-top-170
  margin-top 170px

.viewer
  .wrap_img
    background: #F5F3F3
    display: inline-block
    margin-left: 5px
    margin-top: 5px
    min-height: 80px
    min-width: 12%
    text-align: center
    line-height: 80px
    img
      vertical-align: middle
    img.img_new_animation
      // -webkit-transition: all .1s ease
      // -o-transition: all .1s ease
      // transition: all .1s ease
      -webkit-animation: new-item-animation .4s linear forwards;
      -o-animation: new-item-animation .4s linear forwards;
      animation: new-item-animation .4s linear forwards
  .wrap_inline_block
    display: inline-block
[draggable]
  -moz-user-select: none
  -khtml-user-select: none
  -webkit-user-select: none
  user-select: none
  -khtml-user-drag: element
  -webkit-user-drag: element
@keyframes new-item-animation {
  from {
    opacity: 0;
    -webkit-transform: translateY(-400px);
    -ms-transform: translateY(-400px);
    -o-transform: translateY(-400px);
    transform: translateY(-400px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

@-webkit-keyframes new-item-animation {
  from {
    opacity: 0;
    -webkit-transform: translateY(-400px);
    transform: translateY(-400px)
  }

  to {
    opacity: 1;
    transform : translateY(0)
  }
}

@-o-keyframes new-item-animation {
  from {
    opacity: 0;
    -o-transform: translateY(-400px);
    transform: translateY(-400px)
  }

  to {
    opacity: 1;
    transform : translateY(0)
  }
}
